generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Accepted {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model Form {
  id                String @id @default(auto()) @map("_id") @db.ObjectId
  authorname        String
  extraInformation  String
  responseEmail     String
  title             String
  volume            BigInt
  yearOfPublication String
}
